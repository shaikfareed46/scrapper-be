{
  "name": "url-scrapper",
  "version": "0.0.1",
  "scripts": {
    "prodstart": "export NODE_ENV=production && yarn run start",
    "dev": "NODE_ENV=dev ./node_modules/.bin/nodemon -L -e .ts -w src --exec \"./node_modules/ts-node/dist/bin.js src\"",
    "build": "./node_modules/.bin/tsc --p tsconfig.json",
    "start": "node dist",
    "prestart": "yarn install",
    "prebuild": "yarn install",
    "test": "NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/mocha 'test/*/*.spec.ts'",
    "type-check": "tsc --noEmit",
    "eslint": "./node_modules/.bin/eslint  src/*/*.ts",
    "format": "prettier --write src/*/*.ts",
    "fix-eslint": "./node_modules/.bin/eslint src/*/*.ts --fix --project ./tsconfig.json"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.10",
    "continuation-local-storage": "^3.2.1",
    "express": "~4.17.1",
    "fastest-validator": "^1.5.1",
    "helmet": "^4.1.1",
    "husky": "^4.2.5",
    "log4js": "^2.5.3-3",
    "moment": "^2.29.1",
    "mysql2": "^2.3.3",
    "nanoid": "^3.1.16",
    "request": "^2.85.0",
    "sequelize": "^6.15.1",
    "sequelize-typescript": "^2.1.2",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/assert": "^0.0.31",
    "@types/bluebird": "^3.5.36",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/continuation-local-storage": "^3.2.1",
    "@types/express": "^4.17.12",
    "@types/ioredis": "^4.26.4",
    "@types/json2csv": "^5.0.1",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lru-cache": "^5.1.0",
    "@types/mocha": "^5.2.3",
    "@types/mssql": "^6.0.3",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^10.17.60",
    "@types/request": "^2.47.1",
    "@types/sequelize": "^4.28.11",
    "@types/sinon": "5.0.1",
    "@types/sinon-chai": "^3.2.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validatorjs": "^3.7.1",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "istanbul": "^0.4.5",
    "lint-staged": "^7.1.3",
    "mocha": "^4.0.1",
    "nock": "^9.4.2",
    "nodemon": "^1.12.5",
    "nyc": "^11.4.1",
    "prettier": "1.13.4",
    "proxyquire": "^1.8.0",
    "sinon": "6.1.3",
    "sinon-chai": "^2.14.0",
    "source-map-support": "0.5.6",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "typescript": "^4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run format && yarn run eslint"
    }
  }
}
